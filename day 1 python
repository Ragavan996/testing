new=dt.date(2025,6,9)
print(new)
print("YEAR :",new.year)
print("YEAR :",new.month)
print("YEAR :",new.day)
a=dt.time(10,45,5,45387)
print(a)
print("-----------------------------------------------------")
time=dt.datetime.now()
print(time)
print("-----------------------------------------------------")
